
model expenses {
  id             String     @id @default(uuid()) @db.Uuid
  client_id      String?    @db.Uuid
  user_id        String?
  post_date      DateTime   @default(now()) @db.Timestamptz
  doc_id         String?
  category_id    String?    @db.Uuid
  amount         Decimal    @default(0.00) @db.Decimal(18,4)
  store_name     String?
  store_branch   String?
  order_date     DateTime?  @default(now()) @db.Timestamptz
  order_id       String?
  tax            Decimal    @default(0.00) @db.Decimal(18,4)
  payment_method String?
  is_ocr         Boolean    @default(false)
  is_approved    Boolean    @default(false)
  is_duplicate   Boolean    @default(false)
  approved_by    String?    @db.Uuid
  approved_date  DateTime?  @default(now()) @db.Timestamptz
  metadata       Json?
  status_id      Int        @default(1) @db.SmallInt

  @@map("${TABLE_PREFIX}expenses")
}

